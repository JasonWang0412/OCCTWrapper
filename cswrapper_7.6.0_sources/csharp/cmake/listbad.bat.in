:: Auxiliary: list types not recognized by SWIG, and places where they are used

call "%~dp0env.bat"

cd "%~dp0Wrappers"
for %%a in (SWIGTYPE_p_*.cs) do (

  echo %%a | sed s@'\.cs'@@ | sed s@'SWIGTYPE_p_'@'Not wrapped type: '@

  :: Do not report known cases: Standard_*Stream
  if not %%a == SWIGTYPE_p_Standard_OStream.cs (
    if not %%a == SWIGTYPE_p_Standard_IStream.cs (

      echo Used in:
      grep -l %%~na * | grep -v %%a
      echo.
    )
  )
)

cd "%~dp0"